{
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "camelcase": [2, {"properties": "never"}],
    "no-unused-vars": 0,
    "no-use-before-define": 0,
    "strict": 0,
    "no-shadow": 0,
    "no-multi-spaces": 0,
    "no-underscore-dangle": 0,
    "quotes": [
      2,
      "single"
    ]
  }
}
